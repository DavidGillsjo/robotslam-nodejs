extends ../layout

block content
  .export-container
    .container
      .modal-dialog
        .modal-content
          .modal-header
            h5= `Exporting measurement #${measurement.id}`
          .modal-body
            .progress
              .progress-bar(data-max=points)
          .modal-footer
            span#status Ready
            button.btn.btn-primary(type="button") Start
